@article{hildebrandt_anisotropic_2004,
	title = {Anisotropic filtering of non-linear surface features},
	volume = {23},
	issn = {01677055},
	doi = {10.1111/j.1467-8659.2004.00770.x},
	abstract = {A new method for noise removal of arbitrary surfaces meshes is presented which focuses on the preservation and \${\textbackslash}backslash\$nsharpening of non-linear geometric features such as curved surface regions and feature lines. Our method uses a \${\textbackslash}backslash\$nprescribed mean curvature flow (PMC) for simplicial surfaces which is based on three new contributions: 1. the \${\textbackslash}backslash\$ndefinition and efficient calculation of a discrete shape operator and principal curvature properties on simplicial \${\textbackslash}backslash\$nsurfaces that is fully consistent with the well-known discrete mean curvature formula, 2. an anisotropic discrete \${\textbackslash}backslash\$nmean curvature vector that combines the advantages of the mean curvature normal with the special anisotropic \${\textbackslash}backslash\$nbehaviour along feature lines of a surface, and 3. an anisotropic prescribed mean curvature flow which converges \${\textbackslash}backslash\$nto surfaces with an estimated mean curvature distribution and with preserved non-linear features. Additionally, \${\textbackslash}backslash\$nthe PMC flow prevents boundary shrinkage at constrained and free boundary segments.},
