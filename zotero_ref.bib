@article{gerig_shape_2001,
	title = {Shape analysis of brain ventricles using {SPHARM}},
	volume = {0},
	doi = {10.1109/MMBIA.2001.991731},
	abstract = {Enlarged ventricular size and/or asymmetry have been found markers\${\textbackslash}backslash\$nfor psychiatric illness, including schizophrenia. However, this\${\textbackslash}backslash\$nmorphometric feature is non-specific and occurs in many other brain\${\textbackslash}backslash\$ndiseases, and its variability in healthy controls is not sufficiently\${\textbackslash}backslash\$nunderstood. We studied ventricular size and shape in 3D MRI (N=20) of\${\textbackslash}backslash\$nmonozygotic (N=5) and dizygotic (N=5) twin pairs. Left and right\${\textbackslash}backslash\$nlateral, third and fourth ventricles were segmented from high-resolution\${\textbackslash}backslash\$nT1w SPGR MRI using supervised classification and 3D connectivity.\${\textbackslash}backslash\$nSurfaces of binary segmentations of left and right lateral ventricles\${\textbackslash}backslash\$nwere parametrized and described by a series expansion using spherical\${\textbackslash}backslash\$nharmonics. Objects were aligned using the intrinsic coordinate system of\${\textbackslash}backslash\$nthe ellipsoid described by the first order expansion. The metric for\${\textbackslash}backslash\$npairwise shape similarity was the mean squared distance (MSD) between\${\textbackslash}backslash\$nobject surfaces. Without normalization for size, MZ twin pairs only\${\textbackslash}backslash\$nshowed a trend to have more similar lateral ventricles than DZ twins.\${\textbackslash}backslash\$nAfter scaling by individual volumes, however, the pairwise shape\${\textbackslash}backslash\$ndifference between right lateral ventricles of MZ twins became very\${\textbackslash}backslash\$nsmall with small group variance, differing significantly from DZ twin\${\textbackslash}backslash\$npairs. This finding suggests that there is new information in shape not\${\textbackslash}backslash\$nrepresented by size, a property that might improve understanding of\${\textbackslash}backslash\$nneurodevelopmental and neurodegenerative changes of brain objects and of\${\textbackslash}backslash\$nheritability of size and shape of brain structures. The findings further\${\textbackslash}backslash\$nsuggest that alignment and normalization of objects are key issues in\${\textbackslash}backslash\$nstatistical shape analysis which need further exploration},
	journal = {Proceedings IEEE Workshop on Mathematical Methods in Biomedical Image Analysis (MMBIA 2001)},
	author = {Gerig, G. and Styner, M. and Jones, D. and Weinberger, D. and Lieberman, J.},
	month = aug,
	year = {2001},
	keywords = {decomposition, shape},
	pages = {171--178}
}

@article{stinson_sex_1985,
	title = {Sex differences in environmental sensitivity during growth and development},
	volume = {28},
	issn = {10968644},
	doi = {10.1002/ajpa.1330280507},
	abstract = {Data on sex differences in mortality, morbidity, growth under conditions of environmental stress, and growth responses to environmental improvement are reviewed to test the hypothesis that males are less buffered than females against the environment during growth and development. The hypothesis predicts that males should be more affected by environmental stress, and the strongest support for this is found in studies of the prenatal period. Under stressful conditions, males have higher late fetal mortality than females, and their fetal growth generally has been found to be more retarded. Investigations of postnatal responses to environmental stress have yielded much less consistent results, in large part because of the fact that male children are given preferential treatment in many societies. Results of studies of differences between the sexes in their response to environmental improvement are contradictory. During the prenatal peroid males seem to show a greater response to nutritional supplementation, while postnatal catch-up growth is usually greater in females. Similarly, some investigations report larger secular trends in males, while others have found larger increases in females. These contradictory findings can be reconciled with the hypothesis of greater male environmental sensitivity, but they illustrate the need to obtain more specific information on the environment to which males and females are subject before the hypothesis can be tested adequately.},
	number = {6 S},
	journal = {American Journal of Physical Anthropology},
	author = {Stinson, Sara},
	year = {1985},
	keywords = {Environmental stress, Human growth and development, Mortality, Secular trends, Sex differences, Sexual dimorphism},
	pages = {123--147}
}

@article{zeng_segmentation_1999,
	title = {Segmentation and measurement of the cortex from 3-{D} {MR} images using coupled-surfaces propagation.},
	volume = {18},
	issn = {0278-0062},
	doi = {10.1109/42.811276},
	abstract = {The cortex is the outermost thin layer of gray matter in the brain; geometric measurement of the cortex helps in understanding brain anatomy and function. In the quantitative analysis of the cortex from MR images, extracting the structure and obtaining a representation for various measurements are key steps. While manual segmentation is tedious and labor intensive, automatic reliable efficient segmentation and measurement of the cortex remain challenging problems, due to its convoluted nature. Here we present a new approach of coupled-surfaces propagation, using level set methods to address such problems. Our method is motivated by the nearly constant thickness of the cortical mantle and takes this tight coupling as an important constraint. By evolving two embedded surfaces simultaneously, each driven by its own image-derived information while maintaining the coupling, a final representation of the cortical bounding surfaces and an automatic segmentation of the cortex are achieved. Characteristics of the cortex, such as cortical surface area, surface curvature, and cortical thickness, are then evaluated. The level set implementation of surface propagation offers the advantage of easy initialization, computational efficiency, and the ability to capture deep sulcal folds. Results and validation from various experiments on both simulated and real three-dimensional (3-D) MR images are provided.},
	number = {10},
	journal = {IEEE transactions on medical imaging},
	author = {Zeng, X. and Staib, L. H. and Schultz, R. T. and Duncan, James S.},
	year = {1999},
	pmid = {10628952},
	keywords = {3d segmentation, MRI, by going through the, coupled, get com-, level set, neighboring slices can we, plete information to perform, segmentation, slice by slice, surfaces propagation, volumetric layer},
	pages = {927--937}
}

