	doi = {10.1016/S0196-8858(03)00101-5},
	abstract = {One way to characterize configurations of points up to congruence is by considering the distribution of all mutual distances between points. This paper deals with the question if point configurations are uniquely determined by this distribution. After giving some counterexamples, we prove that this is the case for the vast majority of configurations. In the second part of the paper, the distribution of areas of sub-triangles is used for characterizing point configurations. Again it turns out that most configurations are reconstructible from the distribution of areas, though there are counterexamples. ?? 2003 Elsevier Inc. All rights reserved.},
	number = {4},
	journal = {Advances in Applied Mathematics},
	author = {Boutin, Mireille and Kemper, Gregor},
	year = {2004},
	pages = {709--735}
}

