@incollection{acosta_automatic_2008,
	title = {Automatic delineation of sulci and improved partial volume classification for accurate 3D voxel-based cortical thickness estimation from {MR}},
	volume = {5241 LNCS},
	isbn = {3-540-85987-X},
	abstract = {Accurate cortical thickness estimation in-vivo is important for the\${\textbackslash}backslash\$nstudy of many neurodegenerative diseases. When using magnetic resonance\${\textbackslash}backslash\$nimages (MRI), accuracy may be hampered by artifacts such as partial\${\textbackslash}backslash\$nvolume (PV) as the cortex spans only a few voxels. In zones of opposed\${\textbackslash}backslash\$nsulcal banks (tight sulci) the measurement can be even more difficult.\${\textbackslash}backslash\$nThe aim of this work is to propose a voxel-based cortical thickness\${\textbackslash}backslash\$nestimation method from MR by inte-\${\textbackslash}backslash\$n\${\textbackslash}backslash\$ngrating a mechanism for correcting sulci delineation after an improved\${\textbackslash}backslash\$npartial volume classification. First, an efficient and accurate framework\${\textbackslash}backslash\$nwas developed to enhance partial volume classification with structural\${\textbackslash}backslash\$ninformation. Then, the correction of sulci delineation is performed\${\textbackslash}backslash\$nafter a homotopic thinning of a cost function image. Integrated to\${\textbackslash}backslash\$nour voxel-based cortical thickness estimation pipeline, the overall\${\textbackslash}backslash\$nmethod showed a better estimate of thickness and a high reproducibility\${\textbackslash}backslash\$non real data (R2 {\textbackslash}textgreater 0.9). A quantitative analysis on clinical data\${\textbackslash}backslash\$nfrom an Alzheimer's disease study showed significant differences\${\textbackslash}backslash\$nbetween normal controls and Alzheimer's disease patients.},
