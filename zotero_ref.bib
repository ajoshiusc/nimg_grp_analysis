	keywords = {Adult, Arthritis, Rheumatoid, Carpal Bones, Female, Humans, Image Processing, Computer-Assisted, Male, Wrist Joint, Young Adult},
	pages = {416--426}
}

@article{bergey_long-term_2015,
	title = {Long-term treatment with responsive brain stimulation in adults with refractory partial seizures},
	volume = {84},
	issn = {0028-3878, 1526-632X},
	url = {http://www.neurology.org/content/84/8/810},
	doi = {10.1212/WNL.0000000000001280},
	abstract = {Objective: The long-term efficacy and safety of responsive direct neurostimulation was assessed in adults with medically refractory partial onset seizures.
Methods: All participants were treated with a cranially implanted responsive neurostimulator that delivers stimulation to 1 or 2 seizure foci via chronically implanted electrodes when specific electrocorticographic patterns are detected (RNS System). Participants had completed a 2-year primarily open-label safety study (n = 65) or a 2-year randomized blinded controlled safety and efficacy study (n = 191); 230 participants transitioned into an ongoing 7-year study to assess safety and efficacy.
Results: The average participant was 34 (±11.4) years old with epilepsy for 19.6 (±11.4) years. The median preimplant frequency of disabling partial or generalized tonic-clonic seizures was 10.2 seizures a month. The median percent seizure reduction in the randomized blinded controlled trial was 44\% at 1 year and 53\% at 2 years (p {\textless} 0.0001, generalized estimating equation) and ranged from 48\% to 66\% over postimplant years 3 through 6 in the long-term study. Improvements in quality of life were maintained (p {\textless} 0.05). The most common serious device-related adverse events over the mean 5.4 years of follow-up were implant site infection (9.0\%) involving soft tissue and neurostimulator explantation (4.7\%).
Conclusions: The RNS System is the first direct brain responsive neurostimulator. Acute and sustained efficacy and safety were demonstrated in adults with medically refractory partial onset seizures arising from 1 or 2 foci over a mean follow-up of 5.4 years. This experience supports the RNS System as a treatment option for refractory partial seizures.
Classification of evidence: This study provides Class IV evidence that for adults with medically refractory partial onset seizures, responsive direct cortical stimulation reduces seizures and improves quality of life over a mean follow-up of 5.4 years.},
	language = {en},
	number = {8},
	urldate = {2017-10-05TZ},
	journal = {Neurology},
	author = {Bergey, Gregory K. and Morrell, Martha J. and Mizrahi, Eli M. and Goldman, Alica and King-Stephens, David and Nair, Dileep and Srinivasan, Shraddha and Jobst, Barbara and Gross, Robert E. and Shields, Donald C. and Barkley, Gregory and Salanova, Vicenta and Olejniczak, Piotr and Cole, Andrew and Cash, Sydney S. and Noe, Katherine and Wharen, Robert and Worrell, Gregory and Murro, Anthony M. and Edwards, Jonathan and Duchowny, Michael and Spencer, David and Smith, Michael and Geller, Eric and Gwinn, Ryder and Skidmore, Christopher and Eisenschenk, Stephan and Berg, Michel and Heck, Christianne and Ness, Paul Van and Fountain, Nathan and Rutecki, Paul and Massey, Andrew and O'Donovan, Cormac and Labar, Douglas and Duckrow, Robert B. and Hirsch, Lawrence J. and Courtney, Tracy and Sun, Felice T. and Seale, Cairn G.},
	month = feb,
